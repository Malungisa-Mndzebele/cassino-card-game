{
  "name": "cassino-card-game",
  "version": "1.0.0",
  "description": "Real-time multiplayer Cassino card game for khasinogaming.com",
  "homepage": "https://khasinogaming.com/cassino",
  "scripts": {
    "dev": "node dev-server.js",
    "start": "node frontend/production-server.js",
    "build": "echo 'âœ… Casino Card Game v2.0 - No build step required (vanilla HTML/CSS/JS)'",
    "start:backend": "cd backend && py -3.11 start_production.py",
    "install:backend": "cd backend && py -3.11 -m pip install -r requirements.txt",
    "install:frontend": "npm install",
    "install:all": "npm run install:backend && npm run install:frontend",
    "test": "cd backend && py -3.11 run_simple_tests.py",
    "test:backend": "cd backend && py -3.11 run_simple_tests.py",
    "clean": "del /s /q /f node_modules 2>nul || rmdir /s /q node_modules 2>nul",
    "fresh-install": "npm run clean && npm install",
    "type-check": "tsc --noEmit",
    "lint": "eslint --ext .ts,.tsx .",
    "format": "prettier --write .",
    "deploy:guide": "echo 'ðŸ“– See DEPLOYMENT_GUIDE.md for production deployment instructions'"
  },
